    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.57.0",
