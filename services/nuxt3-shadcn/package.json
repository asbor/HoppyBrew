    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "lucide-vue-next": "^0.553.0",
    "nuxt": "^3.11.2",
    "nuxt-highcharts": "^3.1.1",
    "nuxt-socket-io": "^3.0.13",
    "radix-vue": "^1.9.17",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^3.4.0",
